{
  "name": "my-app-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "run-p build:watch credentials",
    "build": "ng build",
    "test": "ng test --code-coverage",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "build:browser:prod": "ng build --prod",
    "build:browser:serverless": "ng build --prod --base-href /production/",
    "build:serverless": "npm run build:browser:serverless && npm run build:server:serverless",
    "build:prod": "npm run build:client-and-server-bundles && npm run webpack:server",
    "server": "node local.js",
    "build:prod:deploy": "npm run build:prod && npm run deploy",
    "build:serverless:deploy": "npm run build:serverless && npm run deploy",
    "deploy": "serverless deploy",
    "build:server:prod": "ng run myAppAngular:server && webpack --config webpack.server.config.js --progress --colors",
    "build:server:serverless": "ng run myAppAngular:server:serverless && webpack --config webpack.server.config.js --progress --colors",
    "postinstall": "node credentials.js && opencollective postinstall",
    "credentials": "node credentials.js",
    "build:watch": "ng serve",
    "build:dev": "run-p test:watch start",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:client-and-server-bundles": "ng build --prod && ng run myAppAngular:server",
    "test:watch": "ng test --watch --code-coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26",
    "cors": "~2.8.4",
    "cp-cli": "^1.1.0",
    "aws-serverless-express": "^3.2.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "typescript-collections": "^1.3.2",
    "@angular/service-worker": "^6.0.0",
    "@angular/platform-server": "^6.0.0",
    "@angular/cdk": "^6.0.0",
    "@angular/material": "^6.0.0",
    "webpack-cli": "2.1.2",
    "@ngx-translate/core": "^10.0.1",
    "@ngx-translate/http-loader": "^3.0.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.3",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "^5.7.0",
    "typescript": "~3.1.6",
    "ts-loader": "4.2.0",
    "webpack-cli": "2.1.2",
    "opencollective": "^1.0.3",
    "serverless": "1.26.1",
    "serverless-apigw-binary": "^0.4.4",
    "cp-cli": "^1.1.0",
    "cpy-cli": "^1.0.1",
    "decompress": "^4.2.0",
    "decompress-targz": "^4.1.1",
    "express": "^4.15.2",
    "http-server": "^0.10.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "node-wget": "^0.4.2",
    "npm-run-all": "^4.1.2",
    "pre-commit": "^1.2.2",
    "reflect-metadata": "^0.1.10",
    "sinon": "^4.5.0"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/ng-toolkit"
  }
}
